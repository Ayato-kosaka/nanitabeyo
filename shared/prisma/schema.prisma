generator client {
  provider        = "prisma-client-js"
  output          = "../../shared/prisma"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["dev"]
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model backend_event_logs {
  id                String                          @id
  event_name        String?
  error_level       backend_event_logs_error_level?
  function_name     String?
  user_id           String?                         @db.Uuid
  payload           Json?                           @db.Json
  request_id        String?
  created_at        DateTime                        @db.Timestamptz(6)
  created_commit_id String

  @@index([created_at], map: "idx_backend_event_logs_created_at")
  @@index([error_level], map: "idx_backend_event_logs_error_level")
  @@index([request_id], map: "idx_backend_event_logs_request_id")
  @@schema("dev")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
