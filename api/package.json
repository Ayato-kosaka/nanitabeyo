{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "concurrently \"pnpm:nest:dev\" \"pnpm:copy:prisma:watch\"",
    "nest:dev": "nest start api --watch",
    "copy:prisma": "rimraf dist/shared/prisma && cpx \"../shared/prisma/**/*\" dist/shared/prisma",
    "copy:prisma:watch": "npm run copy:prisma && cpx \"../shared/prisma/**/*\" dist/shared/prisma -w -v",
    "build": "run-s build:api copy:prisma",
    "build:api": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/main",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:functional:dish-categories": "ts-node --project tsconfig.json -r tsconfig-paths/register test/functional/v1/dish-categories/runner.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/tasks": "^5.0.0",
    "@google-cloud/vision": "^5.2.0",
    "@googlemaps/places": "^1.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "6.13.0",
    "abort-controller": "^3.0.0",
    "dotenv": "^16.4.7",
    "expo-server-sdk": "^3.15.0",
    "google-auth-library": "^10.2.1",
    "lodash.chunk": "^4.2.0",
    "nanoid": "^3.3.11",
    "nestjs-cls": "^6.0.1",
    "node-fetch": "2",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "shared": "workspace:*",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/node-fetch": "2",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.0",
    "cpx": "^1.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prisma": "6.13.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
